<table>

  <% if @users.nil? %>
      <% @users.each do |user| %>
          <tr>
            <td><%= user.login %></td>
            <td><%= link_to 'Show', user_path(user) %></td>
          </tr>
      <% end %>
  <% else %>
      <tr>
        <td><%= user.login %></td>
        <td><%= link_to 'Show', user_path(user) %></td>
      </tr>
  <% end %>


</table>